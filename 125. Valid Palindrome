class Solution {
    public boolean isPalindrome(String s) {
        s = s.toLowerCase();
        int i = 0, j = s.length() - 1;
        boolean yes = true;
        while(i < j) {
            int asc = (int) s.charAt(i);
            if(!(asc >= 48 && asc <= 57) && !(asc >= 97 && asc <= 122)) {
                i++;
                continue;
            }
            asc = (int) s.charAt(j);
            if(!(asc >= 48 && asc <= 57) && !(asc >= 97 && asc <= 122)) {
                j--;
                continue;
            }
            if(s.charAt(i) == s.charAt(j)) {
                i++;
                j--;
                continue;
            }
            yes = false;
            break;
        }
        return yes;
    }
}
