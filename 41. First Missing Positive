class Solution {
    public int firstMissingPositive(int[] nums) {
        Arrays.sort(nums);
        int s = 1;
        for(int i = 0; i < nums.length; i++) {
            if(i != 0) {
                if(nums[i] == nums[i - 1]) continue;
            }
            if(nums[i] <= 0) continue;
            if(nums[i] != s) return s;
            s++;
        }
        return s;
    }
}
