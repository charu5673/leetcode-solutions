class Solution {
    public int romanToInt(String s) {
        String rm=s;
        int ask=1;
        int dc=0;
            int x=7,y=3,z=8,q=13;
        dc=0;
        int i,n=rm.length();
        int a[]=new int[n];
        for(i=0;i<n;i++)
        {
            switch(rm.charAt(i))
            {
                case 'I':a[i]=1;break;
                case 'V':a[i]=5;break;
                case 'X':a[i]=10;break;
                case 'L':a[i]=50;break;
                case 'C':a[i]=100;break;
                case 'D':a[i]=500;break;
                case 'M':a[i]=1000;break;
            }
            q=z;
            z=y;
            y=x;
            x=a[i];
            if(y==z&&x==y&&q==z)
            {
                dc=-1;
                break;
            }
        }
        for(i=0;i<n-1;i++)
        {
            if(dc==-1)
            break;
            if(a[i]<a[i+1])
            {
                a[i+1]=a[i+1]-a[i];
                a[i]=0;
            }
        }
        for(i=0;i<n;i++)
        {
            if(dc==-1)
            break;
            dc+=a[i];
        }
        return(dc);
    }
}
