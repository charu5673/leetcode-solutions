class Solution {
    public String multiply(String num1, String num2) {
        List<String> ans = new LinkedList<>();
        int n = num1.length();
        int m = num2.length();
        int max = 0;
        for(int i = n - 1; i >= 0; i--) {
            int c = 0;
            String p = "";
            char cur = num1.charAt(i);
            for(int j = 0; j < n - i - 1; j++) p += "0";
            for(int j = m - 1; j >= 0; j--) {
                int d = ((int)cur - 48) * ((int)num2.charAt(j) - 48);
                d += c;
                p += d % 10;
                c = d / 10;
            }
            if(c != 0) p += c;
            ans.add(p);
            if(p.length() > max) max = p.length();
        }
        String res = "";
        int c = 0;
        for(int i = 0; i < max; i++) {
            int sum = 0;
            for(String s : ans) {
                if(s.length() > i) {
                    sum += (int)s.charAt(i) - 48;
                }
            }
            sum += c;
            res = sum % 10 + res;
            c = sum / 10;
        }
        if(c > 0) res = c + res;
        while(res.charAt(0) == '0' && res.length() > 1) res = res.substring(1);
        return res;
    }
}
