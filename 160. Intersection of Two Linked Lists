/**
 * Definition for singly-linked list.
 * public class ListNode {
 *     int val;
 *     ListNode next;
 *     ListNode(int x) {
 *         val = x;
 *         next = null;
 *     }
 * }
 */
public class Solution {
    List<ListNode> v = new LinkedList<>();
    public ListNode getIntersectionNode(ListNode headA, ListNode headB) {
        while(headA != null) {
            v.add(headA);
            headA = headA.next;
        }
        while(headB != null) {
            if(v.contains(headB)) return headB;
            headB = headB.next;
        }
        return null;
    }
}
