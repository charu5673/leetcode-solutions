class Solution {
    public String getBuzz(int n) {
        if(n % 3 == 0 && n % 5 == 0) return "FizzBuzz";
        if(n % 3 == 0) return "Fizz";
        if(n % 5 == 0) return "Buzz";
        return n + "";
    }
    public List<String> fizzBuzz(int n) {
        LinkedList<String> ans = new LinkedList<>();
        for(int i = 1; i <= n; i++) {
            ans.add(getBuzz(i));
        }
        return ans;
    }
}
