class Solution {
    public int majorityElement(int[] nums) {
        if(nums.length==1)
        return nums[0];
        HashMap<Integer,Integer> h=new HashMap<Integer,Integer>();
        int i;
        for(i=0;i<nums.length;i++)
        {
            if(h.containsKey(nums[i]))
            {
                h.put(nums[i],h.get(nums[i])+1);
                if(h.get(nums[i])>(nums.length/2))
                break;
            }
            else
            h.put(nums[i],1);
        }
        return nums[i];
    }
}
