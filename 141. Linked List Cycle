/**
 * Definition for singly-linked list.
 * class ListNode {
 *     int val;
 *     ListNode next;
 *     ListNode(int x) {
 *         val = x;
 *         next = null;
 *     }
 * }
 */
public class Solution {
    List<ListNode> v = new LinkedList<>();
    public boolean hasCycle(ListNode head) {
        if(head == null) return false;
        if(!v.contains(head) && head.next == null) return false;
        else if(v.contains(head)) return true;
        v.add(head);
        return hasCycle(head.next);
    }
}
